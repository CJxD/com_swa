<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

		<field name="user_id" type="text"
			   label="User id"
			   required="true"
			   filter="safehtml"/>

		<field name="sex" type="list"

			   label="Sex"
			   default="none"

			   required="true">
			<option value="None">Rather not say</option>
			<option value="Male">Male</option>
			<option value="Female">Female</option>
		</field>

		<field name="dob" type="calendar" class="inputbox"
			   required="true"

			   default="0000-00-00"

			   label="Dob"
			   filter="safehtml"/>

		<field name="university_id" type="sql" default="1" key_field="id" value_field="name"
			   query="SELECT id, name FROM #__swa_university"

			   label="University"


			   required="true"
			   filter="safehtml"/>

		<field name="course" type="text" size="40" class="inputbox"

			   label="Course"


			   filter="safehtml"/>

		<field name="graduation" type="text" size="40" class="inputbox"

			   label="Graduation Year"


			   filter="safehtml"/>

		<field name="discipline" type="list"

			   label="Discipline"


				>
			<option value="None">None</option>
			<option value="Competition">Competition</option>
			<option value="Wave">Wave</option>
			<option value="Freestyle">Freestyle</option>
		</field>

		<field name="level" type="list"

			   label="Level"


			   required="true">
			<option value="Beginner">Beginner</option>
			<option value="Intermediate">Intermediate</option>
			<option value="Advanced">Advanced</option>
		</field>

		<field name="shirt" type="list"

			   label="Shirt"


			   required="true">
			<option value="XS">XS</option>
			<option value="S">S</option>
			<option value="M">M</option>
			<option value="L">L</option>
			<option value="XL">XL</option>
		</field>

		<field name="econtact" type="text" size="40" class="inputbox"

			   label="Emerg contact"


			   required="true"
			   filter="safehtml"/>

		<field name="enumber" type="text" size="40" class="inputbox"

			   label="Emerg number"


			   required="true"
			   filter="safehtml"/>

		<field name="swahelp" type="list"

			   label="SWA help?"
			   default="None"

			   required="true">
			<option value="None">No thanks</option>
			<option value="Website">Website</option>
			<option value="Events">Events</option>
			<option value="Media">Media / Marketing</option>
		</field>

	</fieldset>

</form>
