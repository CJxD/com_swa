<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

              <field name="id" type="text" default="0" label="COM_SWA_FORM_LBL_DEPOSIT_ID"
            readonly="true" class="readonly"
            description="JGLOBAL_FIELD_ID_DESC" /> 

       <field name="created_by" type="createdby" default="" 
            label="COM_SWA_FORM_LBL_DEPOSIT_CREATED_BY"
            description="COM_SWA_FORM_DESC_DEPOSIT_CREATED_BY"  /> 

       <field name="university_id" type="text" size="40" class="inputbox"
        
            label="COM_SWA_FORM_LBL_DEPOSIT_UNIVERSITY_ID"
            description="COM_SWA_FORM_DESC_DEPOSIT_UNIVERSITY_ID" 
        
        
         required="true" 
        filter="safehtml" /> 

       <field name="time" type="calendar" class="inputbox"
         required="true" 
        
         default="0000-00-00 00:00:00" 
        
            label="COM_SWA_FORM_LBL_DEPOSIT_TIME"
            description="COM_SWA_FORM_DESC_DEPOSIT_TIME" 
        format="%Y-%m-%d %H:%M:%S"
        filter="safehtml" /> 

       <field name="ammount" type="text" size="40" class="inputbox"
        
            label="COM_SWA_FORM_LBL_DEPOSIT_AMMOUNT"
            description="COM_SWA_FORM_DESC_DEPOSIT_AMMOUNT" 
        
        
         required="true" 
        filter="safehtml" /> 

         <field
        name="state"
        type="list"
        label="JSTATUS"
        description="JFIELD_PUBLISHED_DESC"
        class="inputbox"
        size="1"
        default="1">
        <option value="1">JPUBLISHED</option>
        <option value="0">JUNPUBLISHED</option>
    </field> 

                        <field name="checked_out" type="hidden" filter="unset" />
        <field name="checked_out_time" type="hidden" filter="unset" />

	</fieldset>

</form>
