<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

		<field name="id" type="text" default="0" label="COM_SWA_FORM_LBL_EVENTTICKET_ID"
			   readonly="true" class="readonly"
			   description="JGLOBAL_FIELD_ID_DESC"/>

		<field name="created_by" type="createdby" default=""
			   label="COM_SWA_FORM_LBL_EVENTTICKET_CREATED_BY"
			   description="COM_SWA_FORM_DESC_EVENTTICKET_CREATED_BY"/>

		<field name="event_id" type="text" size="40" class="inputbox"

			   label="COM_SWA_FORM_LBL_EVENTTICKET_EVENT_ID"
			   description="COM_SWA_FORM_DESC_EVENTTICKET_EVENT_ID"


			   required="true"
			   filter="safehtml"/>

		<field name="ticket_type_id" type="text" size="40" class="inputbox"

			   label="COM_SWA_FORM_LBL_EVENTTICKET_TICKET_TYPE_ID"
			   description="COM_SWA_FORM_DESC_EVENTTICKET_TICKET_TYPE_ID"


			   required="true"
			   filter="safehtml"/>

		<field name="quantity" type="text" size="40" class="inputbox"

			   label="COM_SWA_FORM_LBL_EVENTTICKET_QUANTITY"
			   description="COM_SWA_FORM_DESC_EVENTTICKET_QUANTITY"


			   required="true"
			   filter="safehtml"/>

		<field name="price" type="text" size="40" class="inputbox"

			   label="COM_SWA_FORM_LBL_EVENTTICKET_PRICE"
			   description="COM_SWA_FORM_DESC_EVENTTICKET_PRICE"


			   required="true"
			   filter="safehtml"/>

		<field
				name="state"
				type="list"
				label="JSTATUS"
				description="JFIELD_PUBLISHED_DESC"
				class="inputbox"
				size="1"
				default="1">
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
		</field>

		<field name="checked_out" type="hidden" filter="unset"/>
		<field name="checked_out_time" type="hidden" filter="unset"/>

	</fieldset>

</form>
